{
  "name": "postgres",
  "version": "1.0.0",
  "description": "test app for working with postgres using node and express",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node ./dist/index.js",
    "dev:server": "nodemon --watch './src/**/*.ts' --exec node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "knex:migrate:make": "knex migrate:make --migrations-directory ./src/migrations -x ts",
    "knex:migrate:latest": "knex --knexfile ./src/db-config/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile ./src/db-config/knexfile.ts migrate:rollback"
  },
  "author": "mark phillips",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/knex": "^0.16.1",
    "@types/node": "^16.11.6",
    "@types/pg": "^8.6.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@types/lodash": "^4.14.177",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^0.95.14",
    "lodash": "^4.17.21",
    "pg": "^8.7.1"
  },
  "type": "module",
  "exports": "./dist/index.ts"
}
